{
  "name": "@vensst/export-file",
  "version": "2.0.0-beta.1",
  "description": "导出excel、pdf和图片",
  "keywords": [
    "@vensst/export-file",
    "@vensst",
    "xlsx",
    "xlsx-js-style",
    "html-to-image",
    "jspdf",
    "file-saver"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "browser": "lib/index.umd.js",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js"
    },
    "./lib/toExcel": {
      "types": "./lib/toExcel.d.ts",
      "import": "./lib/toExcel.esm.js",
      "require": "./lib/toExcel.cjs.js"
    },
    "./lib/toExcel.js": {
      "types": "./lib/toExcel.d.ts",
      "import": "./lib/toExcel.esm.js",
      "require": "./lib/toExcel.cjs.js"
    },
    "./lib/toImage": {
      "types": "./lib/toImage.d.ts",
      "import": "./lib/toImage.esm.js",
      "require": "./lib/toImage.cjs.js"
    },
    "./lib/toImage.js": {
      "types": "./lib/toImage.d.ts",
      "import": "./lib/toImage.esm.js",
      "require": "./lib/toImage.cjs.js"
    },
    "./lib/toPDF": {
      "types": "./lib/toPDF.d.ts",
      "import": "./lib/toPDF.esm.js",
      "require": "./lib/toPDF.cjs.js"
    },
    "./lib/toPDF.js": {
      "types": "./lib/toPDF.d.ts",
      "import": "./lib/toPDF.esm.js",
      "require": "./lib/toPDF.cjs.js"
    }
  },
  "private": false,
  "scripts": {
    "clean": "rimraf lib",
    "build:ts": "tsc --build --clean",
    "build": "npm run clean && npm run build:ts && rollup -c --bundleConfigAsCjs",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "author": "vensst",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vensst/export-file.git"
  },
  "bugs": {
    "url": "https://github.com/vensst/export-file/issues"
  },
  "homepage": "https://github.com/vensst/export-file",
  "dependencies": {
    "file-saver": "2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "xlsx-js-style": "1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup": "^3.29.5",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.8.3"
  }
}
